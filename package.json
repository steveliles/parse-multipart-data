{
  "name": "parse-multipart-data",
  "version": "0.0.0-development",
  "description": "A javascript/nodejs multipart/form-data parser which operates on raw data.",
  "main": "dist/multipart.js",
  "author": "Ignacio Mazzara (nachomazzara@gmail.com)",
  "license": "MIT",
  "scripts": {
    "prebuild": "rm -rf dist",
    "build": "tsc --project tsconfig.json",
    "test": "nyc mocha --require ts-node/register test/*.spec.ts",
    "semantic-release": "semantic-release"
  },
  "nyc": {
    "all": true,
    "include": [
      "src"
    ],
    "extension": [
      ".ts"
    ],
    "reporter": [
      "text-summary"
    ],
    "sourceMap": true,
    "instrument": true
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nachomazzara/parse-multipart-data.git"
  },
  "keywords": [
    "multipart/form-data",
    "form",
    "fileuploader"
  ],
  "bugs": {
    "url": "https://github.com/nachomazzara/parse-multipart-data/issues"
  },
  "homepage": "https://github.com/nachomazzara/parse-multipart-data#readme",
  "dependencies": {
    "@types/node": "^10.11.0",
    "typescript": "^2.9.2"
  },
  "devDependencies": {
    "@types/chai": "^4.1.4",
    "@types/mocha": "^5.2.5",
    "chai": "^4.2.0",
    "mocha": "^5.2.0",
    "nyc": "^13.0.1",
    "prettier": "^1.10.2",
    "semantic-release": "^15.9.16",
    "ts-node": "^7.0.1"
  },
  "prettier": {
    "printWidth": 80,
    "singleQuote": true,
    "semi": false
  }
}
